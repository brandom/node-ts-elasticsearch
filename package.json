    "coveralls": "^3.0.6",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.3.0",
    "reflect-metadata": "^0.1.13"
